{
  "problemMatcher": [
    {
      "owner": "pytest",
      "pattern": [
        {
          "regexp": "^Traceback"
        },
        {
          "regexp": "^\\s*File\\s\"(.*)\",\\sline\\s(\\d+),\\sin\\s(.*)$",
          "file": 1,
          "message": 3,
          "line": 2
        }
      ]
    }
  ]
}
