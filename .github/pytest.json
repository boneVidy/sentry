{
  "problemMatcher": [
    {
      "owner": "pytest",
      "pattern": [
        {
          "regexp": "^\\s*Traceback (most recent call last):"
        },
        {
          "regexp": "^\\s*File\\s\"(.*)\",\\sline\\s(\\d+),\\sin\\s(.*)$",
          "file": 1,
          "line": 2
        },
        {
          "regexp": "^((.*)Error:\\s(.*))$",
          "message": 1,
          "loop": true
        }
      ]
    }
  ]
}
